! Dylans Xresources

! Set the font
URxvt*font: xft:roboto mono:size=10
! URxvt*boldFont: xft:lemon:pixelsize=10:antialias=false

! Disable scrollbar
URxvt.scrollBar: false

! Set urxvt as login shell
URxvt*loginShell: true

! Set terminal padding
URxvt.internalBorder: 15

! Colors {{{

! special
*.foreground:   #f0f0f0
*.background:   #232c33
*.cursorColor:  #f0f0f0

! black
! Second black is grey
*.color0:       #232c33
*.color8:       #3d484d

! red
*.color1:       #99736e
*.color9:       #99736e

! green
*.color2:       #78a090
*.color10:      #78a090

! yellow
*.color3:       #bfb7a1
*.color11:      #bfb7a1

! blue
*.color4:       #7c9fa6
*.color12:      #7c9fa6

! magenta
*.color5:       #BF9C86
*.color13:      #BF9C86

! cyan
*.color6:       #99BFBA
*.color14:      #99BFBA

! white
*.color7:       #f0f0f0
*.color15:      #f0f0f0

! }}}
